{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "./",
  "entryFile": "src/main",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/apigateway/tsconfig.app.json",
    "assets": [
      {
        "include": "proto/admin.proto",
        "outDir": "dist/apps/apigateway/proto",
        "watchAssets": true
      },
      {
        "include": "proto/invites.proto",
        "outDir": "dist/apps/apigateway",
        "watchAssets": true
      },
      {
        "include": "proto/*.proto",
        "outDir": "dist/apps/apigateway",
        "watchAssets": true
      },
      {
        "include": "proto/entities.proto",
        "outDir": "dist/apps/apigateway/proto",
        "watchAssets": true
      }
    ],
    "watchAssets": true
  },
  "monorepo": true,
  "root": "apps/apigateway",
  "projects": {
    "apigateway": {
      "type": "application",
      "root": "apps/apigateway",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/apigateway/tsconfig.app.json",
        "assets": [
          {
            "include": "proto/admin.proto",
            "outDir": "dist/apps/apigateway/proto",
            "watchAssets": true
          },
          {
            "include": "proto/invites.proto",
            "outDir": "dist/apps/apigateway",
            "watchAssets": true
          }
        ],
        "watchAssets": true
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json",
        "assets": ["proto/*.proto"],
        "watchAssets": true
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "mail": {
      "type": "application",
      "root": "apps/mail",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/mail/tsconfig.app.json",
        "assets": ["proto/*.proto"],
        "watchAssets": true
      }
    },
    "admins": {
      "type": "application",
      "root": "apps/admins",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/admins/tsconfig.app.json",
        "assets": ["proto/admin.proto"],
        "watchAssets": true
      }
    },
    "invites": {
      "type": "application",
      "root": "apps/invites",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/invites/tsconfig.app.json",
        "assets": [
          {
            "include": "proto/admin.proto",
            "outDir": "dist/apps/invites/proto",
            "watchAssets": true
          },
          {
            "include": "proto/invites.proto",
            "outDir": "dist/apps/invites",
            "watchAssets": true
          }
        ]
      }
    },
    "files": {
      "type": "application",
      "root": "apps/files",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/files/tsconfig.app.json",
        "assets": ["proto/files.proto"],
        "watchAssets": true
      }
    },
    "school": {
      "type": "application",
      "root": "apps/school",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/school/tsconfig.app.json",
        "assets": [
          {
            "include": "proto/entities.proto",
            "outDir": "dist/apps/school/proto",
            "watchAssets": true
          },
          {
            "include": "proto/school.proto",
            "outDir": "dist/apps/school",
            "watchAssets": true
          }
        ],
        "watchAssets": true
      }
    },
    "teachers": {
      "type": "application",
      "root": "apps/teachers",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/teachers/tsconfig.app.json",
        "assets": [
          {
            "include": "proto/entities.proto",
            "outDir": "dist/apps/teachers/proto",
            "watchAssets": true
          },
          {
            "include": "proto/teachers.proto",
            "outDir": "dist/apps/teachers",
            "watchAssets": true
          }
        ],
        "watchAssets": true
      }
    },
    "school-statistic": {
      "type": "application",
      "root": "apps/school-statistic",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/school-statistic/tsconfig.app.json",
        "assets": [
          {
            "include": "proto/entities.proto",
            "outDir": "dist/apps/school-statistic/proto",
            "watchAssets": true
          },
          {
            "include": "proto/school-statistic.proto",
            "outDir": "dist/apps/school-statistic",
            "watchAssets": true
          }
        ],
        "watchAssets": true
      }
    }
  }
}
